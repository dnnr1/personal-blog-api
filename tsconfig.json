{
  "extends": "./node_modules/gts/tsconfig-google.json",
  "compilerOptions": {
    "target": "es2024",
    "module": "CommonJS",
    "strict": true,
    "esModuleInterop": true,
    "outDir": "./build",
    "skipLibCheck": true,
    "rootDirs": ["."]
  },
  "include": ["**/*.ts", "src/**/*.ts", "**/*.d.ts"],
  "exclude": ["node_modules", "build"]
}
